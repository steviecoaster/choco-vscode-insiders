<?xml version="1.0" encoding="utf-8"?>
<!-- Do not remove this test for UTF-8: if “Ω” doesn’t appear as greek uppercase omega letter enclosed in quotation marks, you should use an editor that supports UTF-8, not this one. -->
<package xmlns="http://schemas.microsoft.com/packaging/2015/06/nuspec.xsd">
  <metadata>
    <id>vscode-insiders</id>
    <version>1.30</version>
    <title>Visual Studio Code Insiders Edition</title>
    <authors>Microsoft</authors>
    <projectUrl>https://code.visualstudio.com/insiders/</projectUrl>
    <packageSourceUrl>https://github.com/steviecoaster/choco-vscode-insiders</packageSourceUrl>
    <tags>vscode-insiders code programming</tags>
    <summary>Bleeding edge build of VSCode</summary>
    <description>Visual Studio Code Insiders edition includes experimental features, along with the latest stable features. Use at your own risk.</description>
    <dependencies>
      <dependency id="dotnet4.5.2" version="4.5.2.20140902" />
      <dependency id="chocolatey-core.extension" version="1.3.3" />
    </dependencies>
  </metadata>
  <files>
    <!-- this section controls what actually gets packaged into the Chocolatey package -->
    <file src="tools\**" target="tools" />
    <!--Building from Linux? You may need this instead: <file src="tools/**" target="tools" />-->
  </files>
</package>